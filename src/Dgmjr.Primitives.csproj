<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFrameworks>netstandard2.0;netstandard2.1;net6.0</TargetFrameworks>
        <!-- <TargetFramework>net7.0</TargetFramework> -->
        <LangVersion>preview</LangVersion>
        <AssemblyName>Dgmjr.Primitives</AssemblyName>
        <PackageId>Dgmjr.Primitives</PackageId>
        <RootNamespace>Dgmjr.Primitives</RootNamespace>
        <Description>A collection of useful data and behavioral value types.</Description>
        <SlnGenSolutionFolder>src</SlnGenSolutionFolder>
        <ProjectGuid>{4E1F4A52-7F97-43E1-AAA6-2EEEE07326E6}</ProjectGuid>
        <ImplicitUsings>enable</ImplicitUsings>
        <CopyLocalLockFileAssemblies>false</CopyLocalLockFileAssemblies>
        <EnablePreviewFeatures>true</EnablePreviewFeatures>
        <NoWarn>$(NoWarn);CA2252;CS8335;</NoWarn>
        <AllowUnsafeBlocks Condition="!$(DeclareConstants.Contains('NET7_0_OR_GREATER'))">true</AllowUnsafeBlocks>
    </PropertyGroup>

    <ItemGroup>
        <Compile Remove="./PreNet7DataStructurePolyfills/*.cs"  Condition="$(DeclareConstants.Contains('NET7_0_OR_GREATER'))" />
        <!-- <PackageReference Include="Dgmjr.Enumerations.CodeGenerator" IncludeAssets="Analyzers;Build" ExcludeAssets="Compile;Runtime" PrivateAssets="All"/> -->
        <NoWarn Include="CS8335" Condition="!$(DeclareConstants.Contains('NET7_0_OR_GREATER'))" />
    </ItemGroup>
</Project>
